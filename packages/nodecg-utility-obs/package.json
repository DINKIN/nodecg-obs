{
  "name": "nodecg-utility-obs",
  "version": "6.0.0",
  "description": "A NodeCG utility that exposes a set of Replicants, Messages, and other hooks for interacting with OBS via obs-websocket",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "test": "nyc ava",
    "build": "npm-run-all build:*",
    "build:schemas": "nodecg schema-types --out-dir types/schemas",
    "build:ts": "tsc -p tsconfig.json"
  },
  "author": {
    "name": "Alex Van Camp",
    "email": "email@alexvan.camp",
    "url": "https://alexvan.camp/",
    "twitter": "vancamp"
  },
  "license": "MIT",
  "dependencies": {
    "clone": "^2.1.2",
    "obs-websocket-js": "^3.0.0"
  },
  "devDependencies": {
    "app-root-path": "^2.2.1",
    "ava": "^0.25.0",
    "json-schema-to-typescript": "^6.1.3",
    "mock-nodecg": "^1.7.0",
    "mockery": "^2.0.0",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "nodecg",
    "utility",
    "helper",
    "obs",
    "obs-websocket",
    "obs-websocket-js"
  ],
  "files": [
    "dist",
    "types",
    "schemas/**/*.json"
  ],
  "engines": {
    "node": ">=7"
  }
}
